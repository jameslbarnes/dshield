{
  "name": "dshield",
  "version": "0.1.0",
  "description": "Serverless functions with cryptographic proof of every external API call",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/runtime/cli.js",
    "dev": "tsx src/runtime/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "proxy": "tsx src/proxy/index.ts",
    "lint": "eslint src tests"
  },
  "bin": {
    "dshield": "./dist/runtime/cli.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "http-proxy": "^1.18.1",
    "node-forge": "^1.3.1",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/http-proxy": "^1.17.14",
    "@types/node": "^20.10.0",
    "@types/node-forge": "^1.3.10",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
